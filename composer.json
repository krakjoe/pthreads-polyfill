{
  "name": "krakjoe/pthreads-polyfill",
  "type": "library",
  "description": "A polyfill for pthreads",
  "keywords": [
    "pthreads",
    "polyfill"
  ],
  "homepage": "http://pthreads.org/",
  "license": "PHP",
  "authors": [
    {
      "name": "Joe Watkins",
      "email": "krakjoe@php.net"
    }
  ],
  "autoload": {
    "psr-0": {
      "": "src"
    },
    "files": [
      "src/constants.php"
    ]
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "archive": {
    "exclude": [
      ".travis.yml",
      "phpunit.xml.dist",
      ".gitignore",
      "example.php",
      "tests"
    ]
  }
}

